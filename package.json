    "type": "git",
    "url": "git+https://github.com/parmsang/gatsby-starter-ecommerce.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "develop": "rm -rf .cache && gatsby develop",
    "build": "rm -rf .cache && gatsby build",
    "deploy":
      "rm -rf .cache && gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi":
      "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
